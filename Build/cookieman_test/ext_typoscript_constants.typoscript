@import 'EXT:cookieman/Configuration/TypoScript/constants.typoscript'

# disable compression and concatenation of CSS and JS files
config {
    compressJs = 0
    compressCss = 0
    concatenateJs = 0
    concatenateCss = 0
}

# test config for bootstrap_package
page.theme.cookieconsent.enable = 0

# test config for cookieman
plugin.tx_cookieman {
    settings {
        links {
            dataProtectionDeclarationPid = 3
            dataProtectionDeclarationAnchor = dataProtectionDeclarationAnchor
            imprintPid = 2
            imprintAnchor = imprintAnchor
        }
        minify = 0
    }
}
